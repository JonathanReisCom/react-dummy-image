{
  "name": "react-dummy-image",
  "version": "1.0.0",
  "description": "NextJS Version of 'react-dummy-image' by Jonathan Reis (https://github.com/JonathanReisCom)",
  "main": "index.jsx",
  "scripts": {
    "export_prod": "yarn a && yarn b && yarn c && yarn d && yarn e && yarn f && yarn g",
    "a": "echo 'Cleaning Babel...' && rm -rf node_modules/.cache/babel-loader || echo 'Nothing to remove.' ",
    "b": "echo 'Installing dependencies...' && yarn install",
    "c": "echo 'Remove old build...' && rm -r out/ || echo 'Nothing to remove.' ",
    "d": "echo 'Remove old build...' && rm -r ../_next || echo 'Nothing to remove.' ",
    "e": "echo 'Building...' && NAMESPACE=prod next build",
    "f": "echo 'Exporting...' && NAMESPACE=prod next export",
    "g": "echo 'Copy folder...' && cp -r out/* ../",
    "---": "---",
    "dev": "NAMESPACE=development next",
    "start": "NAMESPACE=development next start",
    "----": "---",
    "export_mac": "yarn l_a && yarn l_b && yarn l_c && yarn l_d && yarn l_e",
    "l_a": "echo 'Cleaning Babel...' && rm -rf node_modules/.cache/babel-loader || echo 'Nothing to remove.' ",
    "l_b": "echo 'Installing dependencies...' && yarn install",
    "l_c": "echo 'Remove old build...' && rm -r out/ || echo 'Nothing to remove.' ",
    "l_d": "echo 'Building...' && NAMESPACE=export_mac next build",
    "l_e": "echo 'Exporting...' && NAMESPACE=export_mac next export",
    "-----": "---"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonathanReisCom/react-dummy-image.git"
  },
  "keywords": [
    "next",
    "nextjs",
    "react",
    "reactjs",
    "npx",
    "material",
    "material-design",
    "material-kit",
    "material-kit-react",
    "next-material-kit"
  ],
  "author": "Jonathan Reis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JonathanReisCom/react-dummy-image/issues"
  },
  "homepage": "https://jonathanreis.com",
  "dependencies": {
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@zeit/next-sass": "1.0.1",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "2.2.6",
    "firebase": "^7.14.6",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "moment": "2.25.3",
    "next": "^9.4.0",
    "next-compose-plugins": "2.2.0",
    "next-images": "^1.4.0",
    "next-optimized-images": "^2.6.1",
    "next-seo": "^4.4.0",
    "nextjs-redirect": "2.0.2",
    "node-sass": "4.14.1",
    "path": "0.12.7",
    "prop-types": "15.7.2",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-datetime": "2.16.3",
    "react-dom": "^16.13.1",
    "react-dummy-image": "^1.0.0",
    "react-ga": "^2.7.0",
    "react-lazyload": "^2.6.7",
    "react-spring": "^8.0.27",
    "react-swipeable-views": "0.13.9",
    "react-transition-group": "^4.4.1",
    "swr": "^0.2.0",
    "use-global-hook": "^0.1.12",
    "webpack": "4.43.0"
  }
}
